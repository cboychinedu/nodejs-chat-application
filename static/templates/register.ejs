<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <script src="../javascript/ajax-min.js"></script>
    <link rel="stylesheet" href="register.css">
    <link href='https://fonts.googleapis.com/css?family=Poppins' rel='stylesheet'>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title> Register </title>
</head>
<body>
    <!-- Including the navigator -->
    <%- include('navigator.ejs'); %> 

        <div class="register-div">
            <label for="Firstname"> Firstname </label> <br> 
            <input type="text" name="" placeholder="Firstname" id="firstname" required> <br> 

            <label for="Lastname"> Lastname </label> <br> 
            <input type="text" id="lastname" name="lastname" placeholder="Lastname" id="" required><br> 

            <label for="Email"> Email </label> <br> 
            <input type="email" name="email" placeholder="Email" id="email" required> <br> 

            <label for="password"> Password </label> <br> 
            <input type="password" name="password" placeholder="Password" id="password" required> <br> 

            <button type="submit" id="submit-btn" class="submit-btn"> Submit </button> 

        </div>    








    <!-- Including the footer -->
    <%- include('footer.ejs'); %>    
    
    <!-- Adding the javascript -->
    <script src="../javascript/register.js"></script>
</body>
</html>